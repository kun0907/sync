<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Sequence">

	 <select id="getFlowNo" parameterMap="getFlowNoMap" statementType="CALLABLE">
          CALL GETFLOWNO(?,?,?,?)
      </select>
     <parameterMap type="java.util.Map" id="getFlowNoMap">
         <parameter property="entityType" mode="IN" jdbcType="VARCHAR"/>
         <parameter property="flowJudgeValue" mode="IN" jdbcType="VARCHAR"/>
         <parameter property="length" mode="IN" jdbcType="INTEGER"/>
         <parameter property="flowNo" mode="OUT" jdbcType="VARCHAR"/>
     </parameterMap>
</mapper>