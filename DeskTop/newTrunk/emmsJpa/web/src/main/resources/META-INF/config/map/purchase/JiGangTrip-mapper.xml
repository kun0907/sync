<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="JiGangTrip">
    <resultMap type="JiGangTrip" id="jiGangTripMap">
        <id column="id" property="jiGangTripId"/>
        <result column="transport_no" property="transportNo"/>
        <result column="ship_name" property="shipName"/>
        <result column="ocean_vessel" property="oceanVessel"/>
    </resultMap>

    <!-- 插入仓库信息 -->
    <insert id="insert" parameterType="JiGangTrip" useGeneratedKeys="false">
        insert into p_ji_gang_trip (
        transport_no,ship_name,ocean_vessel
        ) values (
        #{transportNo},#{oceanVessel},#{oceanVessel}
        )
    </insert>

</mapper>