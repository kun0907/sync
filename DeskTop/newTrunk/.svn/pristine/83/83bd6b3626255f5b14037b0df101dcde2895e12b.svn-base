package com.dkd.emms.systemManage.bo;

import java.math.BigDecimal;

public class PickNoticeDetail {

	/**
	 *	领料明细id
	 */
	private String pickDetailId;

	/**
	 * 需用计划主单表
	 */
	private DemandPlan demandplan;
	/**
	 * 需用计划明细
	 */
	private DemandDetail demanddetail;
	/**
	 * 领用数量
	 */
	private BigDecimal pickNum;

	/**
	 * 已理货数量
	 */
	private BigDecimal tallyedNum;
	/**
	 * 库存数量
	 */
	private BigDecimal stockNum;
	/**
	 * 对应库存
	 */
	private Stock stock;
	/**
	 * WBS
	 */
	private Project wbs;
	/**
	 * 系统物料
	 */
	private Materials materials;

	/**
	 * 领料主表
	 */
	private PickNotice pickNotice;
	public String getPickDetailId() {
		return pickDetailId;
	}

	public void setPickDetailId(String pickDetailId) {
		this.pickDetailId = pickDetailId;
	}

	public DemandPlan getDemandplan() {
		return demandplan;
	}

	public BigDecimal getTallyedNum() {
		return tallyedNum;
	}

	public void setTallyedNum(BigDecimal tallyedNum) {
		this.tallyedNum = tallyedNum;
	}

	public void setStockNum(BigDecimal stockNum) {
		this.stockNum = stockNum;
	}

	public BigDecimal getStockNum() {
		return stockNum;
	}

	public void setDemandplan(DemandPlan demandplan) {
		this.demandplan = demandplan;
	}

	public DemandDetail getDemanddetail() {
		return demanddetail;
	}

	public void setDemanddetail(DemandDetail demanddetail) {
		this.demanddetail = demanddetail;
	}

	public BigDecimal getPickNum() {
		return pickNum;
	}

	public void setPickNum(BigDecimal pickNum) {
		this.pickNum = pickNum;
	}

	public Project getWbs() {
		return wbs;
	}

	public void setWbs(Project wbs) {
		this.wbs = wbs;
	}

	public Materials getMaterials() {
		return materials;
	}

	public void setMaterials(Materials materials) {
		this.materials = materials;
	}

	public PickNotice getPickNotice() {
		return pickNotice;
	}

	public void setPickNotice(PickNotice pickNotice) {
		this.pickNotice = pickNotice;
	}

	public Stock getStock() {
		return stock;
	}

	public void setStock(Stock stock) {
		this.stock = stock;
	}
}